  function escapeNewline(str){
    str = str.replace(/\n/g, " ");
    str = str.replace(/\r/g, " ");
    return str;
  }

  function navTo(target){
    if( (typeof sforce != 'undefined') && sforce && (!!sforce.one) ) {
      sforce.one.navigateToSObject(target);
    } else {
      window.open('/sfc/servlet.shepherd/version/download/'+target+'?asPdf=false');
    }
  }
