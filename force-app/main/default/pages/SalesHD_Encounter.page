		<script>

			$(document).ready(function () {

				var additionalContacts = {!additionalContactsString
			};
			if (!isEmpty(additionalContacts)) {
				$("#additionalContacts").show();
				for (var contactId in additionalContacts) {
					$("#additionalContacts p").append('<a style="margin-right:10px;" href="/apex/SalesHD_contact?id=' + contactId + '">' + additionalContacts[contactId] + '</a>');
				}
			}

			var additionalTeam = {!additionalTeamString
			};
			if (!isEmpty(additionalTeam)) {
				$("#additionalTeam").show();
				for (var userId in additionalTeam) {
					$("#additionalTeam p").append('<span style="margin-right:10px; text-decoration:underline;">' + additionalTeam[userId] + '</span>');
				}
			}
});

			function isEmpty(obj) {
				for (var key in obj) {
					if (obj.hasOwnProperty(key))
						return false;
				}
				return true;
			}

			function emailSent() {
				$("#emailSent").slideDown().delay(3500).slideUp();
			}

		</script>
