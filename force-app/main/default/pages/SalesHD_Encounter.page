				</div>
			</div> -->

			<c:SalesHD_RedesignNav/>

			<span class="lnr lnr-arrow-left back-btn" onclick="window.history.back();"></span>

			<!-- <div class="header">
				<span class="lnr lnr-arrow-left back-btn" onclick="window.history.back();"></span>
			</div> -->

			<div class="container shadow white-bg">
				<div class="row">
					<apex:outputPanel id="containerBlock" style="width:100%;">
						<apex:form id="encounterForm" style="width:100%;">
							<label>INFORMATION</label>

							<div>
								<label style="display:inline-block;">Created By:</label>
								<p style="display:inline-block;">{!event.CreatedBy.Name}</p>
							</div>

							<apex:outputPanel rendered="{!IF(event.CreatedById<>event.OwnerId, true, false)}">
								<div>
									<label style="display:inline-block;">Assigned To:</label>
									<p style="display:inline-block;">{!event.Owner.Name}</p>
								</div>
							</apex:outputPanel>

							<apex:outputPanel rendered="{!IF(event.SalesHD_Account_Strategy__c!=null, true, false)}">
								<div>
									<label style="display:inline-block;">Related Sales Plan Strategy:</label>
									<a href="apex/SalesHD_SalesPlan?id={!event.SalesHD_Account_Strategy__r.Account_Sales_Plan__r.Parent_Sales_Plan__c}" style="display:inline-block;">
										<b>Sales Plan</b> - {!CASE(MONTH(event.SalesHD_Account_Strategy__r.Account_Sales_Plan__r.Parent_Sales_Plan__r.SalesHD_Plan_Date__c),
										1, "January", 2, "February", 3, "March", 4, "April", 5, "May", 6, "June", 7, "July", 8, "August", 9, "September",
										10, "October", 11, "November", 12, "December", "None")&" "&TEXT(YEAR(event.SalesHD_Account_Strategy__r.Account_Sales_Plan__r.Parent_Sales_Plan__r.SalesHD_Plan_Date__c))}</a>
								</div>
							</apex:outputPanel>

							<div id="additionalTeam" style="display:none;">
								<label style="display:inline-block;">Additional Kindred Team Members:</label>
								<p style="display:inline-block;"></p>
							</div>

							<div>
								<label style="display:inline-block;">Start/End Datetime:</label>
								<p style="display:inline-block;">{!startDateFormated} to {!endDateFormated}</p>
							</div>
							<div>
								<label style="display:inline-block;">Primary Contact:</label>
								<p style="display:inline-block;">
									<a href="/apex/SalesHD_contact?id={!event.WhoId}">{!event.Who.Name}</a>
								</p>
							</div>
							<div id="additionalContacts" style="display:none;">
								<label style="display:inline-block;">Additional Contacts:</label>
								<p style="display:inline-block;"></p>
							</div>
							<div>
								<label style="display:inline-block;">Account:</label>
								<p style="display:inline-block;">
									<a href="/apex/SalesHD_account?id={!event.WhatId}">{!event.What.Name}</a>
								</p>
							</div>
							<div>
								<label style="display:inline-block;">Unit:</label>
								<!-- <p style="display:inline-block;"><a href="/apex/SalesHD_unit?id={!event.SalesHD_Account_Unit__c}">{!event.SalesHD_Account_Unit__r.Unit__r.Name}</a></p> -->
								<p style="display:inline-block;">
									<a href="/apex/SalesHD_unit?id={!event.SalesHD_Account_Unit__c}">{!if(event.SalesHD_Account_Unit__r.Unit__r.Name="Administrative", "Global / Director", event.SalesHD_Account_Unit__r.Unit__r.Name)}</a>
								</p>
							</div>
							<!-- <div>
								<label style="display:inline-block;">Encounter Type:</label>
								<p style="display:inline-block;">{!event.Type}</p>
							</div> -->

							<div>
								<label style="display:inline-block;">Contact Method:</label>
								<p style="display:inline-block;">{!event.SalesHD_Contact_Method__c}</p>
							</div>

							<div>
								<label style="display:inline-block;">Objective:</label>
								<p style="display:inline-block;">{!event.SalesHD_Call_Objective__c}</p>
							</div>

							<div>
								<label style="display:inline-block;">Topic:</label>
								<p style="display:inline-block;">{!event.SalesHD_Topic__c}</p>
							</div>

							<label style="width:100%; border-top:1px solid #ddd;">PREPARATION</label>
							<div>
								<label style="{!IF(ISBLANK(event.SalesHD_Last_Encounter__c), 'display:none;', 'display:inline-block;')}">Last Encounter:</label>
								<p style="{!IF(ISBLANK(event.SalesHD_Last_Encounter__c), 'display:none;', 'display:inline-block;')}">{!event.SalesHD_Last_Encounter__c}</p>
							</div>
							<div>
								<label style="display:inline-block;">Open Ended Questions:</label>
								<p style="display:inline-block;">{!event.SalesHD_Open_Ended_Questions__c}</p>
							</div>
							<div>
								<label style="display:inline-block;">Marketing Collateral:</label>
								<p style="display:inline-block;">{!event.SalesHD_Marketing_Collateral__c}</p>
							</div>
							<div>
								<label style="display:inline-block;">Agenda:</label>
								<p style="display:inline-block;">{!event.SalesHD_Agenda_Preparation__c}</p>
							</div>
							<div>
								<label style="display:inline-block;">Data:</label>
								<p style="display:inline-block;">{!event.SalesHD_Data_Preparation__c}</p>
							</div>

							<div>
								<label style="display:inline-block;">Attachments: </label>
								<apex:repeat value="{!files}" var="f">
									<br/>
									<a href="/sfc/servlet.shepherd/version/download/{!f.Id}" class="blue-link" style="padding-right:10px;">{!f.Title}</a>
									<apex:commandLink action="{!deleteFiles}" styleClass="delete-link" rerender="containerBlock" status="statusSpinner">
										<i class="fas fa-trash-alt"></i> delete
										<apex:param name="fileId" assignTo="{!fileId}" value="{!f.Id}" />
									</apex:commandLink>
								</apex:repeat>

								<apex:actionStatus id="statusSpinner">
									<apex:facet name="start">

										<div class="slds-spinner_container" style="position:fixed; width:100vw; height:100vh;">
											<div class="slds-spinner slds-spinner--large slds-spinner--brand">
												<div class="slds-spinner__dot-a"></div>
												<div class="slds-spinner__dot-b"></div>
