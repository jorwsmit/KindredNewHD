	<body>

		<div class="slds-scope">

			<!-- <div class="nav-container blue-bg">
				<div class="row">
					<ul class="nav-list">
						<li onclick="window.location.href='/apex/saleshd_homepage'">
							<i class="fas fa-home"></i> HOME
