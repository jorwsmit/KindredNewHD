              data+='\n"'+$(this).attr("name")+'":"'+escapeJSON($(this).text())+'",';
            });
            data = data.substring(0, data.length-1);
            data +='\n},\n"hhCR":{';
            $("#hhCR .fields input").each(function(){
              data+='\n"'+$(this).attr("name")+'":"'+escapeJSON($(this).text())+'",';
            });
            data = data.substring(0, data.length-1);
            data +='\n},\n"hpCR":{';
            $("#hpCR .fields input").each(function(){
              data+='\n"'+$(this).attr("name")+'":"'+escapeJSON($(this).text())+'",';
            });
            data = data.substring(0, data.length-1);
            data +='\n},\n"sa":{';
            $("#sa .saFields input").each(function(){
              data+='\n"'+$(this).attr("name")+'":"'+escapeJSON($(this).text())+'",';
            });
            data = data.substring(0, data.length-1);
            data +='\n},\n"af":{';
            $("#af .fields input").each(function(){
              data+='\n"'+$(this).attr("name")+'":"'+escapeJSON($(this).text())+'",';
            });
            data = data.substring(0, data.length-1);
            data +='\n}\n}';
            Sales_FCRcontroller.updateFCR(fcrId, data, function(result, event){
              window.location.href = "{!LEFT($CurrentPage.URL,FIND('/',$CurrentPage.URL,9))}"+'/apex/Sales_summaryFCR?fcrId='+fcrId+'&fcrType='+fcrType;
            }, {escape:false});
          }
          </script>
        </body>
