            $('#users').animate({'left' : "-100%"});
            pageNum=1;
            pageScroll=true;
            $('#selectedPage').show();
            $('#profileIcon').fadeIn();
            $("#pageOutOf").text(pageNum+' of '+$("#selectedPage i").length);
            openPages();
          }

          function openPages(){
            $('#selectedPage i').removeClass('pageSel');
            $('#selectedPage i:nth-child('+pageNum+')').addClass('pageSel');
            $('#'+fcrType+' > #page'+pageNum).show().animate({'left' : "0%"});
            $('#'+fcrType+' > #page'+pageNum+1).show().animate({'left' : "100%"}).promise($("#getLoad").fadeOut());
            $('body').animate({'backgroundColor' : pageColors[pageNum-1]});
            if($(window).width() > 500){
              //$("#leftArrow").fadeIn();
              $("#rightArrow").fadeIn();
            }
          }

          function noAnimations(){
            var i=1;
            var bool = true;
            while(bool && $("#"+fcrType+" > #page"+i).length){
              if($("#"+fcrType+" > #page"+i).is(':animated')) bool = false;
              i++;
            }
            return bool;
          }


          $('body').keydown(function(e){
            if(e.which==37){
              if(noAnimations() && pageScroll && pageNum>1){
                pageNum--;
                right=false;
              }
            } else if(e.which==39){
              if(noAnimations() && pageScroll && pageNum<$("#selectedPage i").length){
                pageNum++;
                right=true;
              }
            } else if(e.which==13){
              if($('#userList li:not(:hidden)').length==1 && !pageScroll)areYouSure();
            }
            changePage();
          });

          $('body').on("swiperight",function(){
            if(pageScroll && pageNum>1){
              pageNum--;
              right=false;
            }
            changePage();
          });

          $('body').on("swipeleft",function(){
            if(pageScroll && pageNum<$("#selectedPage i").length){
              pageNum++;
              right=true;
            }
            changePage();
          });

          function changePageNtimes(n){
            if(n < pageNum){
              right = false;
              n = pageNum - n;
            } else if(n > pageNum){
              right = true;
              n = n - pageNum;
            } else n = 0;
            for(var i=0; i<n; i++){
              if(!right) pageNum--;
              else pageNum++;
              changePage();
            }
          }

          function changePage(){
            $(".pageDropdown").slideUp();
            if(pageNum!=null){
              $('#selectedPage i').removeClass('pageSel');
              $('#selectedPage i:nth-child('+pageNum+')').addClass('pageSel');
              if(!right){
                $('#'+fcrType+' > #page'+pageNum).css({'backgroundColor' : pageColors[pageNum-1]}).show().animate({'left' : "0"});
                $('#'+fcrType+' > #page'+(pageNum+1)).css({'backgroundColor' : pageColors[pageNum]}).show().animate({'left' : "100%"});
              } else{
                $('#'+fcrType+' > #page'+pageNum).css({'backgroundColor' : pageColors[pageNum-1]}).show().animate({'left' : "0%"});
                $('#'+fcrType+' > #page'+(pageNum-1)).css({'backgroundColor' : pageColors[pageNum-2]}).show().animate({'left' : "-100%"});
              }

              $(".pageDropdown ul li").removeAttr("style");
              $(".pageDropdown ul li:nth-child("+pageNum+")").css("text-decoration","underline");

              $("#pageOutOf").text(pageNum+' of '+$("#selectedPage i").length);

              $('body').css({'backgroundColor' : pageColors[pageNum-1]});
            }
          }

          function getFcr(Id, uId){
            var spLoaded = false;
            var fcrLoaded = false;
            userId = uId;
            fcrId = Id;
            Sales_FCRcontroller.getFCR(fcrId, userId, function(result, event){
              var json = JSON.parse(result);
              if(json.completed){
                window.location = '/apex/Sales_summaryFCR?fcrId='+fcrId+'&fcrType='+json.fcrType;
              } else {
                if(json.fcrType == 'hhCR' || json.fcrType == 'hpCR') document.title = 'CR Performance Evaluation';
                else if(json.fcrType == 'fcr' || json.fcrType == 'fcrAF') document.title = 'FCR Performance Evaluation';
                else if(json.fcrType == 'sa') document.title = 'PSA Performance Evaluation';
                else if(json.fcrType == 'af') document.title = 'TAF Performance Evaluation';

                for (var key in json['fields'][json.fcrType]) {
                  if (json['fields'][json.fcrType].hasOwnProperty(key)) {
                    if(json['fields'][json.fcrType][key]!='null' && key.indexOf('_Text')==-1 && key.indexOf('Sales_PRA')==-1){
                      $('div[data-fieldname="'+key+'"]').siblings(".fontZero").find('.ratings[value="'+json['fields'][json.fcrType][key]+'"]').click().trigger('mouseenter');
                    }  else if(json['fields'][json.fcrType][key]!='null' && key.indexOf('Sales_PRA')!=-1){
                      $('div[data-fieldname="'+key+'"]').siblings("ul").find('li:contains("'+json['fields'][json.fcrType][key]+'")').addClass("selectedUl").click();
                    } else if(json['fields'][json.fcrType][key]!='null' && key.indexOf('_Text')!=-1){
                      $('div[data-fieldname="'+key+'"]').siblings(".fieldTitle").find("textarea").text(json['fields'][json.fcrType][key]).keyup();
                    }
                  }
                }
                $(".editBullet").css("opacity","0");
                $("#saveButton").hide();
                fcrId=json.fcrId;
                fcrType = json.fcrType;
                $("#profileIcon > img").attr("src", json.usrPhotoURL);
                $("#modalUsrName").text(json.usrName);
                $("#modalUsrName").attr("href", "/"+userId);
                $("#"+fcrType).show();
                var html = '';
                for(var i=0; i<$('#'+fcrType+' > .section').length; i++){
                  html+='<i class="fa fa-circle inline" aria-hidden="true"></i>\n';
                }
                $('#selectedPage > div').html(html);
                var pageDropdown = '';
                $('#'+fcrType+' .section .pageTitle').each(function(index){
                  if(index==0) pageDropdown+='<li style="text-decoration:underline;" onclick="changePageNtimes('+(index+1)+');">'+$(this).text()+'</li>'
                  else pageDropdown+='<li onclick="changePageNtimes('+(index+1)+');">'+$(this).text()+'</li>'
                });
                $(".pageDropdown ul").html(pageDropdown);
                fcrLoaded = true;
                if(spLoaded) closeSearch();
              }
            }, {escape:false});



            Sales_FCRcontroller.getSP(userId, monthOffset, function(result, event){
              var json = JSON.parse(result);
              $("#modal > .modalBody").html(json.body);
              $("#modal > .modalFooter").html(json.footer);
              spLoaded = true;
              if(fcrLoaded) closeSearch();
            }, {escape:false});

          }

          document.addEventListener('touchmove', function(event) {
            event = event.originalEvent || event;
            if(event.scale > 1) {
              event.preventDefault();
            }
          }, false);

          window.addEventListener("load",function() {
            setTimeout(function(){
              // This hides the address bar:
              window.scrollTo(0, 1);
            }, 0);
          });

          function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
          }

          function openSummary(){
            $('.summaryButton').html('<i class="fa fa-circle-o-notch fa-spin fa-fw"></i>');
            var data='{"fcrType":"'+fcrType+'",\n';
            data+='"fcr":{';
