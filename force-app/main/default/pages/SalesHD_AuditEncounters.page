													<th>CL</th>
													<th>Contact Method</th>
													<th>Topic</th>
													<th>Date</th>
													<th>Open Ended Questions</th>
													<th>Contact</th>
													<th>Account</th>
													<th>Unit</th>
													<th>Marketing Collateral</th>
													<th>Data Preparation</th>
													<th>Agenda Preparation</th>
													<th>Close / Customer Commitment</th>
													<th>Result</th>
													<th>Next Encounter</th>
													<th>Sales Plan</th>
													<th>Link to Encounter</th>
												</tr>
											</thead>
											<tbody>
												<apex:repeat value="{!encounters}" var="e">
													<tr>

														<apex:outputText rendered="{!e.Encounter_Audit__r.Rating__c='Approved'}">
															<td class="text-success">
																<b>{!e.Encounter_Audit__r.Rating__c}</b>
															</td>
														</apex:outputText>
														<apex:outputText rendered="{!e.Encounter_Audit__r.Rating__c='Needs Improvement'}">
															<td class="text-warning">
																<b>{!e.Encounter_Audit__r.Rating__c}</b>
															</td>
														</apex:outputText>
														<apex:outputText rendered="{!e.Encounter_Audit__r.Rating__c='Rejected'}">
															<td class="text-danger">
																<b>{!e.Encounter_Audit__r.Rating__c}</b>
															</td>
														</apex:outputText>
														<apex:outputText rendered="{!e.Encounter_Audit__r.Rating__c!='Approved'
													&& e.Encounter_Audit__r.Rating__c!='Needs Improvement' && e.Encounter_Audit__r.Rating__c!='Rejected'}">
															<td>
																<b>{!e.Encounter_Audit__r.Rating__c}</b>
															</td>
														</apex:outputText>
														<td style="min-width:170px; table-layout:fixed;">
															<apex:inputField style="border:none;" styleClass="form-control audit-picklist" value="{!e.Encounter_Audit__r.Audit_Result__c}"
															 onchange="saveAudit('{!e.Id}', this.value)" rendered="{!NOT(CONTAINS($Profile.Name, 'Associate') || CONTAINS($Profile.Name, 'PAC'))}"
															/>

															<!-- <apex:selectList style="border:none;" styleClass="form-control" size="1" value="{!e.Encounter_Audit__r.Audit_Result__c}"
															 onchange="saveAudit('{!e.Id}', this.value)">
																<apex:selectOptions value="{!auditPicklist}" />
															</apex:selectList> -->

															<apex:outputText rendered="{!CONTAINS($Profile.Name, 'Associate') || CONTAINS($Profile.Name, 'PAC')}">
																{!e.Encounter_Audit__r.Audit_Result__c}
															</apex:outputText>
														</td>
														<apex:outputPanel rendered="{!NOT(CONTAINS($Profile.Name, 'Associate') || CONTAINS($Profile.Name, 'PAC'))}">
															<td onclick="passEncounterId('{!e.Id}', '{!JSENCODE(e.Encounter_Audit__r.Manager_Comment__c)}'); $('#commentModal').modal('show');">
																<apex:outputText rendered="{!e.Encounter_Audit__r.Manager_Comment__c=null}">
																	<i class="fas fa-comment text-primary"></i>
																</apex:outputText>
																{!e.Encounter_Audit__r.Manager_Comment__c}
															</td>
														</apex:outputPanel>

														<apex:outputPanel rendered="{!CONTAINS($Profile.Name, 'Associate') || CONTAINS($Profile.Name, 'PAC')}">
															<td>
																{!e.Encounter_Audit__r.Manager_Comment__c}
															</td>
														</apex:outputPanel>

														<td style="white-space:nowrap;">{!e.Owner.Name}</td>
														<td>{!e.SalesHD_Contact_Method__c}</td>
														<td>{!e.SalesHD_Topic__c}</td>
														<td style="white-space:nowrap;">
															<apex:outputText value="{0,date,MM-dd-yyyy}">
																<apex:param value="{!e.StartDateTime}" />
															</apex:outputText>
														</td>
														<td>{!e.SalesHD_Open_Ended_Questions__c}</td>
														<td>{!e.Who.Name}</td>
														<td>{!e.What.Name}</td>
														<td>
															{!if(e.SalesHD_Account_Unit__r.Unit__r.Name="Administrative", "Global / Director", e.SalesHD_Account_Unit__r.Unit__r.Name)}
														</td>
														<td>{!e.SalesHD_Marketing_Collateral__c}</td>
														<td>{!e.SalesHD_Data_Preparation__c}</td>
														<td>{!e.SalesHD_Agenda_Preparation__c}</td>
														<td>{!e.SalesHD_Close_Customer_Commitment__c}</td>
														<td>{!e.Result__c}</td>
														<td>{!e.SalesHD_Next_Encounter__c}</td>
														<td>
															<apex:outputPanel rendered="{!IF(e.SalesHD_Account_Strategy__c!=null, true, false)}">
																{!CASE(MONTH(e.SalesHD_Account_Strategy__r.Account_Sales_Plan__r.Parent_Sales_Plan__r.SalesHD_Plan_Date__c), 1, "January",
																2, "February", 3, "March", 4, "April", 5, "May", 6, "June", 7, "July", 8, "August", 9, "September", 10, "October",
																11, "November", 12, "December", "None")&" "&TEXT(YEAR(e.SalesHD_Account_Strategy__r.Account_Sales_Plan__r.Parent_Sales_Plan__r.SalesHD_Plan_Date__c))}
															</apex:outputPanel>
														</td>
														<td style="border:none; font-size:20px; padding: 0px 5px !important;">
															<a href="apex/SalesHD_Encounter?id={!e.Id}">
																<i class="fas fa-share"></i>
															</a>
														</td>
													</tr>

												</apex:repeat>
											</tbody>
										</table>
									</div>
								</apex:outputPanel>
								<apex:actionFunction name="saveAudit" action="{!saveAudit}" rerender="filterList">
									<apex:param name="encounterId" value="" assignTo="{!encounterId}" />
									<apex:param name="auditResult" value="" assignTo="{!auditResult}" />
								</apex:actionFunction>



								<div id="commentModal" class="modal" tabindex="-1" role="dialog">
									<div class="modal-dialog" role="document">
										<div class="modal-content">
											<div class="modal-header">
												<h5 class="modal-title">Manager Comment</h5>
											</div>
											<div class="modal-body">
												<apex:inputHidden id="managerEncounterId" value="{!encounterId}" />
												<apex:inputTextarea id="managerComment" styleClass="form-control" value="{!managerComment}" html-maxlength="2048"></apex:inputTextarea>
											</div>
											<div class="modal-footer">
												<apex:commandButton styleClass="btn btn-primary" action="{!saveComment}" value="Save Comment" rerender="filterList" rendered="{!NOT(CONTAINS($Profile.Name, 'Associate') || CONTAINS($Profile.Name, 'PAC'))}"
												/>
												<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
											</div>
										</div>
									</div>
								</div>

								<!-- </ul> -->
							</apex:form>
						</div>
					</div>
				</div>
			</div>


		</div>
