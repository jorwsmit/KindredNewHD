		<script>

			$(document).ready(function () {
				$('#dataTable').DataTable({
					dom: 'Bfrtip',
					fixedHeader: {
						headerOffset: 40,
					},
					paging: false,
					// buttons: {
					// 	dom: {
					// 		button: {
					// 			className: 'btn-primary btn-excel'
					// 		}
					// 	},
					// 	buttons: ['excel']
					// }
				});
			});

			function passEncounterId(encounterId, managerComment) {
				document.getElementById('{!$Component.filterList.managerEncounterId}').value = encounterId;
				document.getElementById('{!$Component.filterList.managerComment}').value = managerComment;
			}

			var green = 'text-success', yellow = 'text-warning', red = 'text-danger';
			var auditResultColorMap = {
				'Approved': green,
				'Asking for business only': red,
				'Duplicate encounter': red,
				'Improper use of field': yellow,
				'Info in summary or agenda does not match objective': yellow,
				'Lacking detail': yellow,
				'Lacking strategy': yellow,
				'Missing agenda': yellow,
				'Missing next steps': yellow,
				'Missing summary': yellow,
				'No data/metrics/collateral used when needed': yellow,
				'Not a sales call': red,
				'Referral managing': red
			};

			$(".audit-picklist").on('click', function () {
				$(this).find("option").each(function (index, value) {
					var color = auditResultColorMap[$(value).val()];
					if (color) $(value).addClass(color);
				});
			});

			// var entries = document.getElementByClassName('{!$Component.picklist}');
			// for (var i = entries.length - 1; i >= 0; i--) {
			// 	if (entries.options[i].text == '') {
			// 		entries.remove(i);
			// 	}
			// }

		</script>

	</body>

